## Contracts env example
# Copy this file to `.env` and fill values.

# Required for Sepolia deployments
SEPOLIA_RPC_URL=
PRIVATE_KEY=

# Optional: for verifying on Etherscan
ETHERSCAN_API_KEY=

# Optional: reuse an existing USDC on Sepolia
# If not provided, deploy_sepolia.js deploys MockUSDC and mints to deployer
USDC_ADDRESS=

# Optional: also create a sample project after deploying registry
CREATE_SAMPLE_PROJECT=false

